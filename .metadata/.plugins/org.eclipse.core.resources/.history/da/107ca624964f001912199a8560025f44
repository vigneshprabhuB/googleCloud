package com.pack.java;

public class Listt {

	public static void main(String[] args) {

		System.out.println(new Listt().calculateSum(5));
		Thread a=new Thread(){
			@Override
			public void run(){
				System.out.println("A");
			}
		};
		new Thread(a).run();
	}

	public int calculateSum(int value) {

		int sum = 1;
		if (value - 1 >= 1) {
			sum = value * calculateSum(value - 1);
		}

		return sum;
	}

}
