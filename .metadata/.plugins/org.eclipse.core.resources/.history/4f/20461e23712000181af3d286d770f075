package com.test.pol;


public class Numbertoword {

int findNumberOfDigit(int number){
	int digit=0;
	if(number>0){
	digit=1;
	while(number>=10){
		number=number/10;
		digit=digit+1;
	}
	}
	return digit;	
}	
	
	
	

   public static void main(String a[]){
	
		int number=9659011;
		
		Numbertoword numbertoword=new Numbertoword();
		int totalDigits=numbertoword.findNumberOfDigit(number);
		for(int j=0;j<totalDigits;j++){
		System.out.println(Math.floor(number/(Math.pow(10, j)) % 10 ));
		}
		
	}

}
