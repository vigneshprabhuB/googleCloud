public class DirPermission {

	public static void main(String a9[]) throws IOException {
	
		
		SimpleDateFormat sdf=new SimpleDateFormat("yyyy-mm-dd");
		sdf.parse("2017-03-17");
		
		
	}

	public void additionOfAllnum(int number) {
	if(number>=0){	
		System.out.println(number);
		int numberr=--number;
		additionOfAllnum(numberr);
		System.out.println("end"+numberr);
	}
	}
}
