package com.test.tryyy;

public class NewThread implements Runnable {

	Thread td = null;
	NewThread(){}
	public NewThread(int priority, int id) {
		td = new Thread(this);
		td.setPriority(priority);
		td.setName(Integer.toString(id));
		td.start();
	}

	@Override
	public void run() {

		for (int i = 0; i <= 10000; i++) {
			System.out.print("");
		}
		System.out.println(td.getName());

	}
	
	public int doSomthing(int i){
		
		if(i<=10){
			
		}
		return i;
	}

	public static void main(String a[]) {

		String bString = "";
		System.out.println(bString);
		for (int j = 0; j < 10; j++) {
			new NewThread(j + 1, j);
		}
		
		NewThread nd=new NewThread();
	      nd.doSomthing(10);
      	

	}

}
