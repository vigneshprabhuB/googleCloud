package com.test.tryyy;

import java.io.IOException;

public class DirPermission {

	public static void main(String a9[]) throws IOException {
		try {
		System.out.println(new DirPermission().additionOfAllnum(3));
		} catch (Exception e) {
			System.out.println(e);
		}
	}

	public int additionOfAllnum(int number) {
		if(!(number<0)){
			number=number+additionOfAllnum(--number);
		}
		return number;
	}
}
