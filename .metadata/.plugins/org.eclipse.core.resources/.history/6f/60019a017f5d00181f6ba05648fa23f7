package com.pack.java;

import java.lang.ref.PhantomReference;
import java.lang.ref.ReferenceQueue;



public class JavaParentClass {


	public static void main(String arg[]) {
		try {
			JavaParentClass a=new JavaParentClass();
			ReferenceQueue<JavaParentClass>rfq=new ReferenceQueue<JavaParentClass>();
			PhantomReference< JavaParentClass>jpc=new PhantomReference<JavaParentClass>(new JavaParentClass(),rfq);
			jpc.get().doSomethig();
		} catch (Exception e) {
			System.out.println(e.getMessage());
		}
	}
	

}
