	public static void main(String a[]) throws IOException {
	InputStream fr=new FileInputStream("F:/assets/16/IMG_20170728_1530431502101523561 - Copy (2).jpg");
	byte []ass=new byte[1024];
	fr.read(ass);
	}
}
