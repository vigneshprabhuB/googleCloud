package com.test.tryyy;

import java.io.IOException;
import java.text.ParseException;

public class DirPermission {

	public static void main(String a9[]) throws IOException, ParseException {
		//System.out.println(new DirPermission().additionOfAllnum(6));
		
		Integer [][]aa=new Integer[3][3];
		aa[0][0]=1;
		aa[0][1]=1;
		System.out.println(aa);
	}

	public int additionOfAllnum(int number) {
		if (number > 0) {
			number = number * additionOfAllnum(--number);
		} else {
			number = 1;

		}
		return number;
	}
}
