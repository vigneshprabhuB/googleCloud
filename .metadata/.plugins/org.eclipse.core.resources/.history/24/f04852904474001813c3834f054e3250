package com.test.tryyy;

import java.io.IOException;
import java.util.Locale;
import java.util.ResourceBundle;

import org.json.JSONException;
import org.json.JSONObject;

public class Statatic {

	public static void main(String a[]) throws IOException, JSONException {
		try {
			JSONObject aa = new JSONObject();
			System.out.println(aa.get("1"));
			Locale locale=new Locale("en", "us");
			ResourceBundle rb=ResourceBundle.getBundle("MasterMessages",locale);
			System.out.println(rb.getObject("SAVE_ASSET_LOCATION"));
		} catch (JSONException e) {
			if (e.getMessage().contains("not found")) {
				System.out.println("please provide correct input...");
			}
		}

	}
}