package com.test.tryyy;

public class Tryyyy implements Runnable {

	Thread td=null;
	public Tryyyy(int priority,String tdname) {
		td=new Thread(this,tdname);
		//td.setPriority(priority);
		td.start();
		
	}
	public void run(){
	
		for(int i=0;i<5;i++){
			
			if(this.td.getName().equals("td2") && i==4 ){
				System.out.println("xxxe");
				this.td.stop();
			}else{
				System.out.println(td.getName()+":::"+i);
			}
		}
	}
	
	
	public static void main(String a[] ){
		new Tryyyy(5, "td1");
		new Tryyyy(7, "td2");

	}

}
