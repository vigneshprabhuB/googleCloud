package com.pack.java;

public class Zimple {

public static void main(String a[]){
	
	String a1=new String("a");
	String a2=new String("a");
	if(a1==a2){
		System.out.println("hai..");
	}else{
		System.out.println("Vaiipu illa raja");
	}
	String a3= new String().intern();
	if(a3==a2){
		System.out.println("hai..");
	}else{
		System.out.println("Vaiipu illa raja");
	}
}

}
