package com.pack.java;


public class FunctionalPrg {
	public static void main(String[] args) {
		
		Numeric q= new Numeric() {
			
			@Override
			public void getNumber(int i) {
i=i+9;				
			}
		};
  
    q.getNumber(8);
	}

}

@FunctionalInterface
interface Numeric {
	int i=0;
	public void getNumber(int i );
	public default void display(){
		System.out.println(i);
	}
}