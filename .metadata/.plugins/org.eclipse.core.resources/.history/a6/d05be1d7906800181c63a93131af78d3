package com.test.tryyy;

import java.sql.DriverManager;

public class Statatic {

	static {
		System.out.println("static block");
	}

	public static void hai() {
		int i = 0;
		System.out.println("hai static...");

	}

	public void haii() {

		System.out.println("haii static...");
		Statatic.hai();
	}

	public static void main(String a[]) throws ClassNotFoundException,
			InstantiationException, IllegalAccessException {
		Class c = Class.forName("com.test.tryyy.Statatic");
		System.out.println(c.newInstance().hashCode());

	}

}
