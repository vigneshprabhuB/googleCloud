package com.test.pol;

public class ThreadSample implements Runnable {
	
	public void run(){
		try {	
			for(int j=0;j<5;j++){
	System.out.println("HAI");
	Thread.currentThread().sleep(100);
			}
		
	} catch (InterruptedException e) {
		// TODO Auto-generated catch block
		e.printStackTrace();
	}
	}

}
