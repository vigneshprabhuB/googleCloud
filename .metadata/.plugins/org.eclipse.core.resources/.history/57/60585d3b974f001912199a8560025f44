package com.pack.java;


public class Listt {

	public static void main(String[] args) {
     Thread tr=new Thread("T1"){
    	 @Override
    	 public void run(){
    		 new Listt().calculateSum(1000);
    	 }
     };
     Thread tr2=new Thread(){
    	 @Override
    	 public void run(){
    		 new Listt().calculateSum(100);
    	 }
     };
tr2.setPriority(1);
tr.setPriority(10);
tr2.run();
tr1.run();

	}

	public int calculateSum(int value) {

		int sum = 1;
		if (value - 1 >= 1) {
			sum = value * calculateSum(value - 1);
		}
        System.out.println(sum);
		return sum;
	}

}
